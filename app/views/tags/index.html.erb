<h1>All Tags</h1>

<ul id="tags">
    <% @tags.each do |tag| %>
        <li>
        <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Are you sure you want to delete this tag?"} %>
        <%= link_to tag.name, tag_path(tag) %>
        </li>
    <% end %>
</ul>

<%= link_to "<< All Articles", articles_path %>